.appointmsg_all {
	width: 100%;
}

.appointmsg_title {
	color: #9B9B9B;
	width: 100%;
	padding: 0px 15px;
	line-height: 40px;
	height: 40px;
}

.mui-btn-danger,
.mui-btn-negative,
.mui-btn-red {
	background-color: red;
}

.mui-slider-handle {
	color: #4A4A4A;
}

.mui-slider-handle img {
	width: 18px;
	vertical-align: middle;
	margin-top: -3px;
	margin-right: 15px;
}

.mui-table-view-cell:after {
	background-color: #ddd;
}

.mui-table-view:before {
	background-color: #ddd;
}

.mui-table-view:after {
	background-color: #ddd;
}

.addcar_bc {
	width: 100%;
	background-color: #FAFAFA;
	padding: 15px;
}

.addcar {
	width: 150px;
	text-align: center;
	border: 1px dashed #ddd;
	height: 40px;
	line-height: 38px;
	border-radius: 20px;
	margin: 0 auto;
	color: #aaa;
}

.appointmsg_form_lf {
	margin-left: 15px;
	width: 30%;
	float: left;
	height: 40px;
	line-height: 40px;
	text-align: left;
}

.appointmsg_form_lf span{
	color: red;
	margin-right: 5px;
}

.appointmsg_form_rg {
	width: 60%;
	float: left;
}
.appointmsg_xy{
	line-height: 30px;
	padding: 5px 15px;
	color: #898989;
	margin-top: 20px;
}
.appointmsg_xy img{
	width: 18px;
	margin-right: 5px;
	vertical-align: middle;
	margin-top: -3px;
}
.appointmsg_xy span{
	color: #007AFF;
}
.appointmsg_sure{
	background-color: #4C72FB;
	width: calc(100% - 30px);
	margin-left: 15px;
	border-radius: 5px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	color: #fff;
	margin-bottom: 20px;
}
.appointmsg_sure:active{
	background-color: #2750CA;
}
