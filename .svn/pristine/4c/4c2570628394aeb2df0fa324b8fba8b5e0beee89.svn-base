package com.eqy.web.pojo;
import java.io.Serializable;

/**
 * @ClassName: PermissionBean
 * @Description: 用户权限表 T_CCS_PERMISSION
 * @author luoyb
 * @date 2017-8-2 下午5:08:57
 * 
 */
public class PermissionBean implements Serializable
{
    /**
     * 序列化
     */
    private static final long serialVersionUID = 7192256921195334227L;
    private String fPermissionCode;// 功能/权限编码
    private String fPermissionName;// 功能/权限代号
    private String fCreateTime;// 创建时间
    private String fPermissionGrade;// 权限等级（1：模块 2：模块下功能）
    private String fParPenCode;// 父模块权限编码
    private String fModuleName;// 模块名称
    private String fRoleDefault;// 角色默认拥有
    private String fState;// 是否启用（0启用 1未启用）
    public String getfState()
    {
        return fState;
    }
    public void setfState(String fState)
    {
        this.fState = fState;
    }
    public String getfPermissionCode()
    {
        return fPermissionCode;
    }
    public void setfPermissionCode(String fPermissionCode)
    {
        this.fPermissionCode = fPermissionCode == null ? null : fPermissionCode.trim();
    }
    public String getfPermissionName()
    {
        return fPermissionName;
    }
    public void setfPermissionName(String fPermissionName)
    {
        this.fPermissionName = fPermissionName == null ? null : fPermissionName.trim();
    }
    public String getfCreateTime()
    {
        return fCreateTime;
    }
    public void setfCreateTime(String fCreateTime)
    {
        this.fCreateTime = fCreateTime == null ? null : fCreateTime.trim();
    }
    public String getfPermissionGrade()
    {
        return fPermissionGrade;
    }
    public void setfPermissionGrade(String fPermissionGrade)
    {
        this.fPermissionGrade = fPermissionGrade == null ? null : fPermissionGrade.trim();
    }
    public String getfParPenCode()
    {
        return fParPenCode;
    }
    public void setfParPenCode(String fParPenCode)
    {
        this.fParPenCode = fParPenCode == null ? null : fParPenCode.trim();
    }
    public String getfModuleName()
    {
        return fModuleName;
    }
    public void setfModuleName(String fModuleName)
    {
        this.fModuleName = fModuleName == null ? null : fModuleName.trim();
    }
    public String getfRoleDefault()
    {
        return fRoleDefault;
    }
    public void setfRoleDefault(String fRoleDefault)
    {
        this.fRoleDefault = fRoleDefault == null ? null : fRoleDefault.trim();
    }
}